@model Consulta
   

@{
    Layout = "../Shared/_Layout.cshtml";
    ViewData["Title"] = "VerConsultas";
}

<h2>Consultas</h2>




<table id="tabela" class="display" style="width:100%">
    <thead>
        <tr>
            <th>Médico</th>
            <th>Especialidade</th>
            <th>Consultório</th>
            <th>Endereço</th>
            <th>Data</th>
            <th>Horário</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>Dra. Ana Paula</td>
            <td>Cardiologista</td>
            <td>Albert Einstein dos Pobres</td>
            <td>Tem q ver isso ai cara</td>
            <td>02/05/2025</td>
            <td>14:00</td>
        </tr>
    </tbody>
</table>



    @section Scripts {
        <script>
                   $(document).ready(function () {
            $('#tabela').DataTable({
                language: {
                    url: "https://cdn.datatables.net/plug-ins/2.2.2/i18n/pt-BR.json"
                }
            });
        });

    </script>
}

